{
  "name": "S3Images",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Key": {
      "type": "string",
      "required": true
    },
    "LastModified": {
      "type": "date",
      "required": true
    },
    "ETag": {
      "type": "string",
      "required": true
    },
    "Size": {
      "type": "number",
      "required": true
    },
    "StorageClass": {
      "type": "string",
      "required": true
    },
    "ExifApertureValue": {
      "type": "string",
      "required": false
    },
    "ExifExposureTime": {
      "type": "string",
      "required": false
    },
    "ExifFocalLength": {
      "type": "string",
      "required": false
    },
    "ExifFlash": {
      "type": "string",
      "required": false
    },
    "ExifIsoSpeedRating": {
      "type": "number",
      "required": false
    },
    "ExifMeteringMode": {
      "type": "string",
      "required": false
    },
    "ExifCameraModel": {
      "type": "string",
      "required": false
    },
    "ExifDateTime": {
      "type": "string",
      "required": false
    },
    "ExifDescription": {
      "type": "string",
      "required": false
    },
    "ExifLocation": {
      "type": "string",
      "required": false
    },
    "ExifKeywords": {
      "type": "string",
      "required": false
    },
    "ExifAuthor": {
      "type": "string",
      "required": false
    },
    "ExifCopyright": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {
    "importS3Data": {
      "isStatic": true,
      "accepts": [
        {
          "arg": "urlEnpoint",
          "type": "string",
          "required": true,
          "description": "Amazon S3 URL endpoint."
        }
      ],
      "returns": [
        {
          "arg": "totalRecordsImported",
          "type": "number",
          "root": false,
          "description": "Total amount of records imported"
        },
        {
          "arg": "totalRecordsFound",
          "type": "number",
          "root": false,
          "description": "Total amount of records found"
        }
      ],
      "description": "Import S3 Data (XML) from URL",
      "http": [
        {
          "path": "/imports3data",
          "verb": "post"
        }
      ]
    }
  }
}
